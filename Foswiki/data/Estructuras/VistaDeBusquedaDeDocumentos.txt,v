if ($("#cbexternos:checked").val() == "externos") {
      $("#tablaexternos").show();          
      var parametrosDocExt = { skin: 'text', nombre: $('[name=nombre]').val().replace(cleanStringregExp, '?'), descripcion: $('[name=descripcion]').val().replace(cleanStringregExp, '?'), proceso: $('[name=proceso]').val()};
